{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
      "plugin:vue/essential",
      "airbnb-base"
    ],
    "parserOptions": {
        "parser": "babel-eslint",
        "ecmaVersion": 2016,
        "sourceType": "module"
    },
    "plugins": [
      "vue"
    ],
    "rules": {
      "no-console": 0,
      "no-unused-vars": 0,
      "comma-dangle": 0,
      "no-param-reassign": 0,
      "max-len": 0,
      "no-restricted-globals": 0,
      "guard-for-in": 0,
      "no-restricted-syntax": 0,
      "no-multi-assign": 0,
      "no-nested-ternary": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-unresolved": 0,
      "func-names": 0
    }
}